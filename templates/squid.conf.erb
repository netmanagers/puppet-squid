# This file is managed by Puppet. DO NOT EDIT.

<% if scope.lookupvar('squid::visible_hostname') != '' -%>
visible_hostname "<%= scope.lookupvar('squid::visible_hostname') %>"
<% end -%>
http_port <%= scope.lookupvar('squid::squid_ip') %> <%= scope.lookupvar('squid::squid_port') %>

cache_peer <%= scope.lookupvar('squid::cache_peer') %> sibling <%= scope.lookupvar('squid::sibling') %> proxy-only name=<%= scope.lookupvar('squid::sibling_name') %>


include /etc/squid/conf.d/*.conf


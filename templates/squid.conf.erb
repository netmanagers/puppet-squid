cache_peer <%= scope.lookupvar('squid::cache_peer') %> sibling <%= scope.lookupvar('squid::sibling') %> proxy-only name=<%= scope.lookupvar('squid::sibling_name') %>

cache_mem <%= scope.lookupvar('squid::cache_mem') %>

cache_swap_low <%= scope.lookupvar('squid::cache_swap_low') %> 
cache_swap_high <%= scope.lookupvar('squid::cache_swap_high') %>

maximum_object_size <%= scope.lookupvar('squid::maximum_object_size') %>

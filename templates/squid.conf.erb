shutdown_lifetime <%= scope.lookupvar('squid::shutdown_lifetime') %>

buffered_logs <%= scope.lookupvar('squid::buffered_logs') %>

pipeline_prefetch <%= scope.lookupvar('squid::pipeline_prefetch') %>
client_persistent_connections <%= scope.lookupvar('squid::client_persistent_connections') %>
dead_peer_timeout <%= scope.lookupvar('squid::dead_peer_timeout') %>

quick_abort_min <%= scope.lookupvar('squid::quick_abort_min') %>
quick_abort_max <%= scope.lookupvar('squid::quick_abort_max') %>
quick_abort_pct <%= scope.lookupvar('squid::quick_abort_pct') %>

via <%= scope.lookupvar('squid::via') %>
forwarded_for <%= scope.lookupvar('squid::forwarded_for') %>

hierarchy_stoplist <%= scope.lookupvar('squid::hierarchy_stoplist') %>

cache_dir <%= scope.lookupvar('squid::cache_dir') %>

